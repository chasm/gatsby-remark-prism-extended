{
  "name": "gatsby-remark-prismjs",
  "description": "Extends gatsby-remark-prismjs to include line numbering and code block line links.",
  "version": "0.1.0",
  "author": "Charles F. Munat <chas@munat.com>",
  "dependencies": {
    "babel-runtime": "^6.26.0",
    "parse-numeric-range": "^0.0.2",
    "prismjs": "^1.10.0",
    "ramda": "^0.25.0",
    "unist-util-visit": "^1.3.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "cross-env": "^5.1.3",
    "remark": "^9.0.0"
  },
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "prismjs",
    "remark",
    "line numbering",
    "code block links"
  ],
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "build": "babel src --out-dir . --ignore __tests__",
    "watch": "babel -w src --out-dir . --ignore __tests__",
    "prepublish": "cross-env NODE_ENV=production npm run build"
  }
}
